{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "pruebamongododgers"
		},
		"AzureDataLakeStorageXX_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageXX'"
		},
		"MongoConn_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MongoConn'"
		},
		"NeonConn_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'NeonConn'"
		},
		"AzureDataLakeStorageXX_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://preliminar1.dfs.core.windows.net/"
		},
		"MongoConn_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "test"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "pruebamongododgers_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageXX_accountKey": {
						"value": "[parameters('AzureDataLakeStorageXX_accountKey')]"
					},
					"MongoConn_connectionString": {
						"value": "[parameters('MongoConn_connectionString')]"
					},
					"NeonConn_connectionString": {
						"value": "[parameters('NeonConn_connectionString')]"
					},
					"AzureDataLakeStorageXX_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageXX_properties_typeProperties_url')]"
					},
					"MongoConn_properties_typeProperties_database": {
						"value": "[parameters('MongoConn_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}